  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:local": "next build && next start",
    "clean": "rm -rf .next node_modules package-lock.json",
    "reset": "npm run clean && npm install",
    "build:pages": "npx @cloudflare/next-on-pages",
    "build:cloudflare": "npx @cloudflare/next-on-pages",
    "preview": "npx wrangler pages dev .vercel/output/static",
    "lint": "next lint",
    "lint:ci": "next lint --max-warnings 0",
    "env:check": "node scripts/env-check.js",
    "quality": "npm run lint:ci && npm run build",
    "verify": "npm run lint:ci && npm run build && echo 'Run: npm run start (then use SMOKE.md checklist)'"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
